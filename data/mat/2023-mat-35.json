  "question": [
    "If <katex>$|\\vec{a}\\times\\vec{b}|^{2}+|\\vec{a}\\cdot\\vec{b}|^{2}=144$</katex> and <katex>$|\\vec{a}|=4$</katex> then <katex>$|\\vec{b}|$</katex> is equal to"
  ],
  "choices": [
    [
      "<katex>$8$</katex>"
    ],
    [
      "<katex>$4$</katex>"
    ],
    [
